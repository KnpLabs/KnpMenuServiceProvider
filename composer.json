{
    "name": "knplabs/knp-menu-silex",
    "type": "library",
    "description": "Integration of knplabs/knp-menu into Silex",
    "keywords": ["menu", "tree", "silex"],
    "homepage": "https://knplabs.com",
    "license": "MIT",
    "authors": [
        {
            "name":     "KnpLabs",
            "homepage": "https://knplabs.com"
        },
        {
            "name":     "Christophe Coevoet",
            "email":    "stof@notk.org"
        },
        {
            "name":     "Community",
            "homepage": "https://github.com/KnpLabs/KnpMenuSilexProvider/contributors"
        }
    ],
    "require": {
        "php":  ">=5.3.0",
        "knplabs/knp-menu": "^2.2",
        "silex/silex": "^1.3"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.3",
        "twig/twig": "^1.16|^2.0"
    },
    "suggest": {
        "twig/twig": "for the TwigRenderer and the integration with your templates"
    },
    "autoload": {
        "psr-4": { "Knp\\Menu\\Silex\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "Knp\\Menu\\Silex\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
